{
  "name": "words",
  "version": "0.0.0",
  "private": true,
  "devDependencies": {
    "prettier": "2.0.5"
  },
  "scripts": {
    "postinstall": "gem install jekyll bundler --conservative && bundle install",
    "format": "prettier --write **/*.{md,scss}",
    "start": "bundle exec jekyll serve --watch"
  },
  "prettier": {
    "printWidth": 60,
    "trailingComma": "none",
    "arrowParens": "avoid"
  }
}
